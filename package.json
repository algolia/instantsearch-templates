{
  "name": "instantsearch-templates",
  "version": "1.0.0",
  "description": "Example templates for various InstantSearch projects",
  "private": true,
  "scripts": {
    "isjs": "cd src/InstantSearch.js && npm start",
    "ris": "cd 'src/React InstantSearch' && npm start",
    "ais": "cd 'src/Angular InstantSearch' && npm start",
    "vis": "cd 'src/Vue InstantSearch' && npm start",
    "jsclient": "cd 'src/JavaScript client' && npm start",
    "jshelper": "cd 'src/JavaScript helper' && npm start",
    "lint": "eslint .",
    "test": "npm run lint"
  },
  "workspaces": ["src/*"],
  "devDependencies": {
    "babel-eslint": "8.2.2",
    "eslint": "4.17.0",
    "eslint-config-algolia": "13.0.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-prettier": "2.6.0",
    "prettier": "1.10.2"
  },
  "engines": {
    "node": "^9.6.1"
  },
  "renovate": {
    "extends": ["config:js-app", "algolia"]
  }
}
